{
  "manifest_version": 2,
  "name": "yo",
  "short_name": "yo",
  "description": "reflection app",
  "version": "0.0.1",
  "minimum_chrome_version": "38",

  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  },

"permissions": ["tabs","webview","activeTab"],

  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  }
}
